{
  "manifest_version": 3,
  "name": "Chromium System-Info",
  "description": "Your System-Info",
  "version": "1.0.0",
  "icons": {
    "128": "logo.png"
  },
  "action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "sw.js"
  },
  "permissions": [
    "activeTab",
     "system.cpu",
      "system.memory",
       "system.storage"
      ]
}
